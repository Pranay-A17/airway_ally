# Environment Configuration for Airway Ally
# Copy this file to .env and fill in your actual values

# Aviation Stack API Key (get from https://aviationstack.com/)
AVIATION_STACK_API_KEY=your_aviation_stack_api_key_here

# Firebase Configuration (optional - uses default if not set)
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_PROJECT_ID=airway-ally

# Environment (development/production)
FLUTTER_ENV=development 